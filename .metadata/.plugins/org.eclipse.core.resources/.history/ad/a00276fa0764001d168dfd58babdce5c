package maze;
import graph.Vertex;

public abstract class MazeBox implements Vertex{
	private int x;
	private int y;
	private String label;
	
	public MazeBox(int x1, int y1) {
		x=x1;
		y=y1;
	}
	
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	public String getLabel() {
		return label;
	}
	
}
